# Parkealo Development Environment Variables
# Copy this file to .env and update the values as needed

# Application Environment
NODE_ENV=development

# Database Configuration
POSTGRES_DB=parkealo_dev
POSTGRES_USER=parkealo_user
POSTGRES_PASSWORD=parkealo_password
POSTGRES_PORT=5432

# Redis Configuration
REDIS_PASSWORD=redis_password
REDIS_PORT=6379

# RabbitMQ Configuration
RABBITMQ_USER=parkealo_user
RABBITMQ_PASSWORD=parkealo_password
RABBITMQ_VHOST=parkealo_vhost
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672

# Service Ports
API_GATEWAY_PORT=3000
USER_SERVICE_PORT=3001
FRONTEND_PORT=3002
NGINX_PORT=8080

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRATION=15m
JWT_REFRESH_EXPIRATION=7d

# API Configuration
API_BASE_URL=http://localhost:3000
FRONTEND_URL=http://localhost:3002

# Email Configuration (Optional - for development)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your-email@gmail.com
EMAIL_PASSWORD=your-app-password

# File Upload Configuration
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=combined
